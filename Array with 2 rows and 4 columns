#include <stdio.h>

int main(void) {
    int m[2][4] = {
        {1, 2, 3, 7},   // Added 7 as the new element in row 1
        {4, 5, 6, 8}    // Added 8 as the new element in row 2
    };

    for (int i = 0; i < 2; i++) {            // Loop through rows
        for (int j = 0; j < 4; j++) {        // Loop through 4 columns now
            printf("%d%c", m[i][j], (j == 3) ? '\n' : ' ');
        }
    }

    return 0;
}
